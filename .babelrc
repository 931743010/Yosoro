{
  "presets": [
    ["env", {
      "targets": {
        "node": 8,
        "modules": false
      }
    }],
    "flow"
  ],
  "env": {
    "main": {
      "presets": [
        ["env", {
          "targets": {
            "node": 8,
            "modules": false
          }
        }],
        "flow"
      ]
    },
    "renderer": {
      "presets": [
        ["env", {
          "targets": {
            "chrome": 61,
            "modules": false
          },
          "loose": true
        }],
        "react",
        "react-optimize",
        "stage-0",
        "flow"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-async-to-generator",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    },
    "test": {
      "presets": [["env"], "react", "flow"]
    }
  },
  "plugins": ["transform-runtime"]
}
